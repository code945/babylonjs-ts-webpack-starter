{
    "name": "babylonjs-ts-webpack-starter",
    "version": "1.0.0",
    "description": "a starter project for babylonjs using typescript + webpack",
    "main": "index.js",
    "scripts": {
        "check": "tsc --noEmit --pretty",
        "check:watch": "npm run check -- --watch",
        "build:types": "tsc --emitDeclarationOnly",
        "dev": "webpack-dev-server --progress --config build/webpack.dev.js",
        "build": "webpack --progress --config build/webpack.prod.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/code945/babylonjs-ts-webpack-starter.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/code945/babylonjs-ts-webpack-starter/issues"
    },
    "homepage": "https://github.com/code945/babylonjs-ts-webpack-starter#readme",
    "devDependencies": {
        "@babel/core": "7.12.10",
        "@babel/plugin-proposal-class-properties": "7.12.1",
        "@babel/plugin-proposal-decorators": "^7.12.12",
        "@babel/plugin-proposal-object-rest-spread": "7.12.1",
        "@babel/preset-env": "7.12.11",
        "@babel/preset-typescript": "7.12.7",
        "autoprefixer": "^9.1.5",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "file-loader": "^5.0.2",
        "fork-ts-checker-webpack-plugin": "6.0.7",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "imports-loader": "^0.8.0",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.2",
        "postcss-loader": "^3.0.0",
        "style-loader": "^0.23.0",
        "ts-loader": "^5.0.0",
        "typescript": "^3.0.3",
        "webpack": "^4.17.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.7"
    },
    "dependencies": {
        "@babylonjs/core": "^4.0.3",
        "@babylonjs/loaders": "^4.0.3",
        "@babylonjs/materials": "^4.0.3",
